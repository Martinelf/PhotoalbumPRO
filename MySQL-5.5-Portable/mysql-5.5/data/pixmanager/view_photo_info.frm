source=SELECT\n	  `p`.`photoID` AS `photoID`,\n	  `p`.`file_path` AS `file_path`,\n	  `p`.`creation_date` AS `creation_date`,\n	  `l`.`location_name` AS `location_name`,\n	  GROUP_CONCAT(DISTINCT ifnull(`pe`.`name`, \'\') ORDER BY `pe`.`name` ASC SEPARATOR \', \') AS `people_names`,\n	  GROUP_CONCAT(DISTINCT ifnull(`t`.`tag_name`, \'\') ORDER BY `t`.`tag_name` ASC SEPARATOR \', \') AS `tag_names`,\n	  `p`.`description` AS `description`,\n	  `p`.`isFavorite` AS `isFavorite`\n	FROM (((((`photos` `p`\n	  LEFT JOIN `locations` `l`\n	    ON ((`p`.`locationID` = `l`.`locationID`)))\n	  LEFT JOIN `photo_people` `pp`\n	    ON ((`p`.`photoID` = `pp`.`photoID`)))\n	  LEFT JOIN `people` `pe`\n	    ON ((`pp`.`personID` = `pe`.`personID`)))\n	  LEFT JOIN `photo_tags` `pt`\n	    ON ((`p`.`photoID` = `pt`.`photoID`)))\n	  LEFT JOIN `tags` `t`\n	    ON ((`pt`.`tagID` = `t`.`tagID`)))\n	GROUP BY `p`.`photoID`,\n	         `p`.`creation_date`,\n	         `l`.`location_name`,\n	         `p`.`description`,\n	         `p`.`isFavorite`
